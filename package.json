{
	"name": "nativeWeb2",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "npm run web",
		"ios": "react-native run-ios",
		"test": "jest",
		"lint": "eslint .",
		"build:web": "HOST=localhost npm run webpack -- --env.mode production",
		"webpack": "webpack --config web/webpack/webpack.config.js",
